{
  "files.exclude": {
    // root directories
    ".dart_tool": true,
    ".idea": true,
    "android": true,
    "build": true,
    "ios": true,
    "linux": true,
    "macos": true,
    "web": true,
    "windows": true,
    // root files
    ".firebaserc": true,
    ".flutter-plugins": true,
    ".flutter-plugins-dependencies": true,
    ".metadata": true,
    "analysis_options.yaml": true,
    "app.iml": true,
    "firebase.json": true,
    "pubspec.lock": true,
    "firebase-debug.log": true,
    "firestore.indexes.json": true,
    "firestore.rules": true,
    // firebase functions
    "functions/node_modules": true,
    "functions/.gitignore": true,
    "functions/package-lock.json": true,
    // ...
    ".firebase": true,
    "public": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#bfa3cb",
    "activityBar.background": "#bfa3cb",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#7a6a45",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#bfa3cb",
    "statusBar.background": "#a983b8",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#9263a5",
    "statusBarItem.remoteBackground": "#a983b8",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#a983b8",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#a983b899",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#a983b8"
}
